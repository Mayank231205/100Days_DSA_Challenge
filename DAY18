#include <stdio.h>

int main() {
    int n;

    printf("Enter size: ");
    scanf("%d", &n);

    int arr[100];

    printf("Enter elements:\n");
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int k;
    printf("Enter k: ");
    scanf("%d", &k);

    k = k % n;

    int temp[100];

    for(int i = 0; i < k; i++)
        temp[i] = arr[n - k + i];

    for(int i = 0; i < n - k; i++)
        temp[k + i] = arr[i];

    printf("Rotated array:\n");
    for(int i = 0; i < n; i++)
        printf("%d ", temp[i]);

    return 0;
}
